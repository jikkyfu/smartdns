	option name 'Google'
	option ip 'https://8.8.4.4/dns-query'
	option type 'https'
	option exclude_default_group '1'
	option server_group 'oversea'
	
config server
	option enabled '1'
	option type 'udp'
	option name 'Google6'
	option ip '2001:4860:4860::8888'
	option server_group 'oversea'
	option exclude_default_group '1'
	option use_proxy '1'

config server
